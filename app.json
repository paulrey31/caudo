{
	"expo": {
		"name": "caudo",
		"slug": "caudo",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/puzzleIcon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"UIEdgeInsets": false,
				"UIViewControllerBasedStatusBarAppearance": true
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/puzzleIcon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.anonymous.caudo"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/puzzleIcon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
